{
    "name": "noc",
    "displayName": "Noc",
    "publisher":"mortim",
    "description": "Highlighting support for the Noc language.",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.57.0"
    },
    "icon":"assets/icon.png",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "noc",
            "aliases": ["Noc", "noc"],
            "extensions": [".noc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "noc",
            "scopeName": "source.noc",
            "path": "./syntaxes/noc.tmLanguage.json"
        }]
    },
    "repository": {
      "type":"git",
      "url": "https://github.com/noc-lang/vscode-noc.git"
    }
}
